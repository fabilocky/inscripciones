<html>
  <head>

  <link href="//maxcdn.bootstrapcdn.com/bootstrap/4.1.1/css/bootstrap.min.css" rel="stylesheet" id="bootstrap-css">
<script src="//maxcdn.bootstrapcdn.com/bootstrap/4.1.1/js/bootstrap.min.js"></script>
<script src="//cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<!------ Include the above in your HEAD tag ---------->

  </head>
<body id="LoginForm">
<div class="container">
<h1 class="form-heading">login Form</h1>
<div class="login-form">
<div class="main-div">
    <div class="panel">
   <h2>Admin Login</h2>
   <p>Please enter your email and password</p>
   </div>
    <form id="Login" name="form" method="post">

        <div class="form-group">
                    <label for="form_nombre" class="col-sm-12 control-label">Nombre</label>
                    <div class="col-sm-12">
                        {{ form_widget(form.nombre) }}
                    </div>
                </div>
                <div class="form-group">
                    <label for="form_apellido" class="col-sm-12 control-label">Apellido</label>
                    <div class="col-sm-12">
                        {{ form_widget(form.apellido) }}
                    </div>
                </div>
                <div class="form-group">
                    <label for="form_email" class="col-sm-12 control-label">Email</label>
                    <div class="col-sm-12">
                        {{ form_widget(form.email) }}
                    </div>
                </div>
                <div class="form-group">
                    <label for="form_telefono" class="col-sm-12 control-label">Teléfono</label>
                    <div class="col-sm-12">
                        {{ form_widget(form.telefono) }}
                    </div>
                </div>
                <div class="form-group">
                    <label for="form_jurisdiccionOrganismo" class="col-sm-12 control-label">Jurisdicción/Organismo</label>
                    <div class="col-sm-12">
                        {{ form_widget(form.jurisdiccionOrganismo) }}
                    </div>
                </div>
                <div class="form-group">
                    <label for="form_tareas" class="col-sm-12 control-label">Tareas</label>
                    <div class="col-sm-12">
                        {{ form_widget(form.tareas) }}
                    </div>
                </div>
                <div class="form-group">
                    <label for="form_localidad" class="col-sm-12 control-label">Localidad</label>
                    <div class="col-sm-12">
                        {{ form_widget(form.localidad) }}
                    </div>
                </div>
                <div class="form-group">
                    <label for="form_provincia" class="col-sm-12 control-label">Provincia</label>
                    <div class="col-sm-12">
                        {{ form_widget(form.provincia) }}
                    </div>
                </div>
                <button type="submit" class="btn btn-primary">Inscribirme</button>
            {{ form_end(form) }} <!-- /form -->        

    </form>
    </div>
</div></div></div>


</body>
</html>
